{
  "version": 2,
  "builds": [
    {
      "src": "src/api/api.ts",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/src/api/api.ts"
    },
    {
      "src": "/painel",
      "dest": "/public/index.html"
    },
    {
      "src": "/styles.css",
      "dest": "/public/styles.css"
    },
    {
      "src": "/script.js",
      "dest": "/public/script.js"
    }
  ]
}