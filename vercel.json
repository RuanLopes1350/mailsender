{
  "version": 2,
  "builds": [
    {
      "src": "dist/api/api.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/painel/(.*)",
      "dest": "/public/$1"
    },
    {
      "src": "/painel",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/api/api.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
