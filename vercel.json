{
  "version": 2,
  "builds": [
    {
      "src": "src/api/api.ts",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "src/api/api.ts"
    },
    {
      "src": "/painel",
      "dest": "/public/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ]
}